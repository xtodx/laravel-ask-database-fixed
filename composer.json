{
    "name": "xtodx/laravel-ask-database",
    "description": "Use natural language and AI to query your database from Laravel",
    "keywords": ["xtodx", "laravel", "laravel-ask-database"],
    "homepage": "https://github.com/xtodx/laravel-ask-database-fixed",
    "license": "MIT",
    "authors": [
        { "name": "Marcel Pociot", "email": "marcel@beyondco.de", "role": "Developer" },
        { "name": "Anton Kovalenko", "email": "tonylovepony@gmail.com", "role": "Fix" }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "openai-php/laravel": "^0.15",
        "spatie/laravel-package-tools": "^1.20",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^3.0",
        "phpunit/phpunit": "^11.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.5",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Xtodx\\Oracle\\": "src",
            "Xtodx\\Oracle\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xtodx\\Oracle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xtodx\\Oracle\\OracleServiceProvider"
            ],
            "aliases": {
                "Oracle": "Xtodx\\Oracle\\Facades\\Oracle"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
